<!doctype html>
<html lang="en">

<head>
    <meta charset='utf-8'>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Faculty of Computer Technology and Sciences</title>
</head>

<body>
    {# header #}
    <div class="container">
        <header class="blog-header py-3">
            <div class="row flex-nowrap justify-content-between align-items-center">
                <div class="col-md-2">
                    {# <img class="img-fluid" src="./res/sample.png" alt="Logo"> #}
                    <img src="./res/sample.png" alt="Logo">
                </div>
                <div class="col-md-8 text-center">
                    FACULTY OF COMPUTER TECHNOLOGY & SCIENCES
                </div>
                <div class="col-md-2">
                    <img class="img-fluid" src="res/logo2.jpg" alt="Logo">
                </div>
            </div>
        </header>

        <div class="nav-scroller py-1 mb-2">
            <nav class="nav d-flex justify-content-between">
                <a class="p-2 text-muted" href="/">Home</a>
                <a class="p-2 text-muted" href="#">About Us</a>
                <a class="p-2 text-muted" href="#">Faculty</a>
                <a class="p-2 text-muted" href="#">Courses</a>
                <a class="p-2 text-muted" href="#">Gallery</a>
                <a class="p-2 text-muted" href="#">Facilities</a>
            </nav>
        </div>
    </div>

    {# main #}
    <div class="container">
        <div class="row mb-2">


            {# {{> leftside}} #}
            <div class="col-md-3">
                <div class="position-sticky">
                    <div class="p4 ">
                        <img src="./res/image1.jpg" class="img-thumbnail " alt="COMDT MCTE IMAGE">
                        <p>LT Gen xxx</p>
                    </div>
                    <div class="p4">
                        <div class="login">
                            <h4>Login</h4>

                            <div class="mb-3 row w-100">
                                <form method="POST">
                                    <input type="text" name="username" placeholder="Username">
                                    <input type="password" name="password" placeholder="Password">
                                    <button type="submit" class="btn btn-primary mb-3">Log in!</button>
                                    {% if login_failed %}
                                    <div class="alert alert-danger" role="alert">
                                        Wrong username/password
                                    </div>
                                    {% endif %}
                                </form>
                            </div>
                            {# <div class="mb-3 row w-100">
                                <label for="email" class="col-sm-4 col-form-label">Email</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="email">
                                </div>
                            </div>
                            <div class="mb-3 row w-100">
                                <label for="inputPassword" class="col-sm-4 col-form-label">Password</label>
                                <div class="col-sm-8">
                                    <input type="password" class="form-control" id="inputPassword">
                                </div>
                            </div> #}
                            {# <button type="submit" class="btn btn-primary mb-3">Login</button> #}
                        </div>
                    </div>
                </div>
            </div>


            <div class="col-md-6">
                {# <h1>{{header}}</h1>
                <form method="POST">
                    <input type="text" name="username" placeholder="Username">
                    <input type="password" name="password" placeholder="Password">
                    <button type="submit">Log in!</button>
                </form> #}
                {% block new_user %}
                 
                {% endblock new_user %}

                {# {{> carousel_images}} #}

                {# {{> news}} #}
            </div>

            {# {{> rightside}} #}
            <div class="col-md-3">
                <div class="position-sticky">
                    <div class="p4 ">
                        <img src="res/logo2.jpg" class="img-thumbnail " alt="CDR FCTS">
                        <p>PR14 xxx</p>
                    </div>
                    <div class="p4">
                        <div class="container search">
                            <h4 class="search-text">Search Here</h4>
                            <div class="d-flex justify-content-left h-100" style="padding-top:3%">
                                <div class="searchbar">
                                    <input class="search_input" type="text" name="" placeholder="Search...">
                                    <a href="#" class="search_icon"><i class="fas fa-search"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>

    {# footer #}
    <div class="container">
        <footer class="row row-cols-5 py-5 my-5 border-top">
            <div class="col">
                <h5>
                    Important links
                </h5>
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            Army Portal
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            Artract
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            Army cypergroup
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            NCTE
                        </a>
                    </li>

                </ul>
            </div>

            <div class="col">
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a href="#" class="nav-link">
                            Important Advisories
                        </a>

                        <a href="#" class="nav-link">
                            Notification regarding banned apps
                        </a>

                        <a href="#" class="nav-link">
                            ACSP-2019
                        </a>
                    </li>
                </ul>
            </div>

            <div class="col">
                <ul class="nav flex-column">
                    Website developed in Rust by:
                    <li>
                        Name 1
                    </li>
                    <li>
                        Name 2
                    </li>
                </ul>
            </div>
        </footer>
    </div>

</body>






</html>